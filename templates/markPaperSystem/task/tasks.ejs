<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE9">
  <meta content="yes" name="apple-mobile-web-app-capable">
  <meta content="yes" name="apple-touch-fullscreen">
  <meta content="telephone=no,email=no" name="format-detection">
  <title>判卷任务列表</title>
  <link rel="stylesheet" href="/assets/vendor/pagination/jquery.pagination.css">
  ${require("html-loader?interpolate!../_assets.html")}
  <script src="/assets/vendor/pagination/jquery.pagination-1.2.1.js"></script>
</head>

<body>
${require("html-loader?interpolate!../_menuBar.html")}
<div id="main">
  <ul id="myTab" class="nav nav-tabs">
    <li class="active">
      <a href="#doing" data-toggle="tab" class="tabDoing">待完成任务</a>
    </li>
    <li>
      <a href="#done" data-toggle="tab" class="tabDone">已完成任务</a>
    </li>
    <li>
      <button class="btn btn-primary btn-other-size" id="refresh">手动刷新阅卷任务</button>
    </li>
  </ul>
  <div id="myTabContent" class="tab-content">
    <div class="tab-pane fade in active" id="doing">
      <div class="container table-cont" id='table-cont'>
        <table id="doing_table" class="display table table-striped" border="1">
          <thead>
          <tr>
            <th>序号</th>
            <th>任务批号</th>
            <th>阅卷状态</th>
            <th>总题数</th>
            <th>已阅</th>
            <th>未阅</th>
            <th>任务下发时间</th>
            <th>最新上传时间</th>
            <th>操作</th>
          </tr>
          </thead>
          <tbody></tbody>
        </table>
      </div>
      <div class="pageFoot">
        <div class="box">
          <select id="selectPage">
            <option value="10">10</option>
            <option value="20">20</option>
            <option value="50">50</option>
            <option value="100">100</option>
          </select>
          <div id="pagination3" class="page fl">
            <div class="ui-pagination-container">
            </div>
          </div>
          <div id="record">
            <span>显示<span id="start">1</span>-<span id="end">10</span>, 共</span>
            <span id="totalCount"></span>
            <span>记录</span>
          </div>
        </div>
      </div>
    </div>

    <div class="tab-pane fade" id="done">
      <div class="container table-cont" id='table-cont2'>
        <table id="done_table" class="display table table-striped" border="1">
          <thead>
          <tr>
            <th>序号</th>
            <th>任务批号</th>
            <th>阅卷状态</th>
            <th>总题数</th>
            <th>已阅</th>
            <th>未阅</th>
            <th>任务下发时间</th>
            <th>最新上传时间</th>
            <th>操作</th>
          </tr>
          </thead>
          <tbody></tbody>
        </table>
      </div>
      <div class="pageFoot">
        <div class="box">
          <select id="selectPage2">
            <option value="10">10</option>
            <option value="20">20</option>
            <option value="50">50</option>
            <option value="100">100</option>
          </select>
          <div id="pagination4" class="page fl">
            <div class="ui-pagination-container">
            </div>
          </div>
          <div id="record">
            <span>显示<span id="startTab">1</span>-<span id="endTab">10</span>, 共</span>
            <span id="totalCount2"></span>
            <span>记录</span>
          </div>
        </div>
      </div>
    </div>

  </div>
</div>
${require("html-loader?interpolate!../_statusBar.html")}
</body>
</html>
